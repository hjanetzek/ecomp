		<short xml:lang="pl">Inicjuj</short>
		<short xml:lang="pt">Iniciar</short>
		<short xml:lang="pt_BR">Iniciar</short>
		<short xml:lang="ru">Инициировать</short>
		<short xml:lang="sv">Initiera</short>
