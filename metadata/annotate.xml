		<default>
		    <button>&lt;Super&gt;&lt;Alt&gt;Button1</button>
		</default>
	    </option>
	    <option type="action" name="draw">
		<short>Draw</short>
		<short xml:lang="hu">Rajzolás</short>
		<short xml:lang="pl">Rysuj</short>
